<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".LastTimeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/LastTimeTheme">
        <activity
            android:name=".eventinstancedetails.EventInstanceDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".eventdetails.EventDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".view.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.SplashScreenActivity"
            android:exported="false" />
        <activity android:name=".view.EventInstanceActivity" />
        <activity android:name=".view.EventActivity" />
        <activity android:name=".view.EditEventActivity" />
        <activity android:name=".view.MainActivity" />
        <activity android:name=".view.EventListActivity" />
    </application>

</manifest>